{
	"name": "obsidian-bases-paginator",
	"version": "1.0.0",
	"description": "Adds a paginated table view with quick filtering to Obsidian Bases",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"fmt": "prettier --write \"src/**/*.ts\"",
		"fmt:check": "prettier --check \"src/**/*.ts\""
	},
	"keywords": [],
	"author": "sean2077",
	"license": "0-BSD",
	"devDependencies": {
		"@eslint/js": "latest",
		"@types/node": "latest",
		"esbuild": "latest",
		"eslint": "latest",
		"eslint-plugin-obsidianmd": "latest",
		"globals": "latest",
		"jiti": "latest",
		"obsidian": "latest",
		"prettier": "latest",
		"tslib": "latest",
		"typescript": "latest",
		"typescript-eslint": "latest"
	}
}
